{
  "name": "mage",
  "description": "MAGE game development platform for Node.js",
  "version": "1.3.1",
  "author": "Wizcorp K.K. <info@wizcorp.jp>",
  "homepage": "https://wizcorp.jp/mage/",
  "bugs": {
    "mail": "info@wizcorp.jp",
    "url": "http://github.com/mage/mage/issues"
  },
  "types": "./mage.ts",
  "engines": {
    "node": "^4 || ^6 || ^7 || ^8",
    "typescript": "^2.2"
  },
  "scripts": {
    "dev:setup": "node ./scripts/githooks.js",
    "test:outdated": "npm outdated --color always --silent || true",
    "test:security": "retire && npm run test:outdated",
    "test:lint": "eslint --ext .js,.json .",
    "test:code-analysis": "run-s test:lint test:security",
    "test:unit": "mocha ./test/index.js",
    "test:unit:filter": "npm run test:unit -- --grep",
    "test:unit-rebuild": "npm rebuild && npm run test:unit",
    "test": "run-s test:lint test:unit test:security",
    "postinstall": "node ./scripts/install.js",
    "preversion": "npm run docs:build && git add ./docs",
    "report:coveralls": "istanbul cover _mocha --root lib --include-all-sources --report lcovonly --dir coveralls -- ./test/index.js && cat ./coveralls/lcov.info | coveralls && rm -rf ./coveralls",
    "report:coverage": "istanbul cover _mocha --root lib --include-all-sources --report html --dir coverage-report -- ./test/index.js",
    "report:complexity:create-config": "yaml2json .eslintrc.yml > .eslintrc",
    "report:complexity:create-report": "plato -r -d complexity-report -e .eslintrc ./lib ./test ./scripts",
    "report:complexity:cleanup-config": "rm .eslintrc",
    "report:complexity": "run-s report:complexity:*",
    "report": "run-p report:complexity report:coverage",
    "docs:build": "run-s docs:build:userguide docs:build:api",
    "docs:build:userguide": "node ./scripts/compile-docs",
    "docs:build:api": "./node_modules/.bin/typedoc --hideGenerator --out docs/api --includeDeclarations --excludeExternals --theme minimal --mode file --readme ./docs-sources/api/Readme.md --sourcefile-url-prefix 'https://github.com/mage/mage/tree/master/'",
    "clean:deps": "node ./scripts/rmrf.js node_modules",
    "clean:report": "node ./scripts/rmrf.js coverage-report complexity-report",
    "clean:docs": "node ./scripts/rmrf.js docs",
    "clean": "npm run clean:deps && npm run clean:report"
  },
  "bin": {
    "mage": "./bin/mage",
    "ts-mage": "./bin/ts-mage"
  },
  "dependencies": {
    "@types/commander": "2.9.1",
    "app-module-path": "2.2.0",
    "async": "0.9.0",
    "chalk": "1.1.3",
    "codependency": "2.0.1",
    "commander": "2.9.0",
    "epipebomb": "1.0.0",
    "forwarded-for": "1.0.1",
    "function-arguments": "1.0.8",
    "handlebars": "4.0.6",
    "heapdump": "0.3.9",
    "http-close": "0.0.2",
    "js-yaml": "3.8.4",
    "json5": "0.5.1",
    "jsonlint": "1.6.2",
    "localstash": "0.2.0",
    "memoizee": "0.4.4",
    "memwatch-next": "0.3.0",
    "mime": "1.3.6",
    "mkdirp": "0.5.1",
    "multipart-parser": "0.0.2",
    "netmask": "1.0.6",
    "node-uuid": "1.4.7",
    "object-sizeof": "1.2.0",
    "panopticon": "0.2.0",
    "prettyjson": "1.2.1",
    "rimraf": "2.6.1",
    "rumplestiltskin": "0.1.0",
    "safe-timers": "1.0.1",
    "semver": "5.3.0",
    "source-map": "0.1.31",
    "tabalot": "0.8.0",
    "wizcorp-deep-copy.js": "0.1.0",
    "wizcorp-timer.js": "0.2.0",
    "ws": "1.1.1",
    "xpipe": "1.0.5"
  },
  "optionalPeerDependencies": {
    "consul": "~0.29.0",
    "graylog2": "~0.1.3",
    "mysql": "^2.4.1",
    "couchbase": "^2.1.6",
    "memcached": "~0.2.6",
    "redis": "~0.9.0",
    "manta": "~1.0.1",
    "sqlite3": "~3.1.8",
    "memorystream": "~0.2.0",
    "aws-sdk": "~1.15.0",
    "es": "~0.4.4",
    "node-zookeeper-client": "~0.2.0",
    "mdns": "~2.3.3",
    "zmq": "~2.15.3",
    "ldapjs": "~0.7.1"
  },
  "devDependencies": {
    "coveralls": "2.13.1",
    "cross-env": "4.0.0",
    "eslint": "3.19.0",
    "eslint-plugin-json": "1.2.0",
    "eslint-plugin-mocha": "4.9.0",
    "git-message": "1.0.3",
    "istanbul": "0.4.5",
    "mktemp": "0.4.0",
    "mocha": "3.4.1",
    "mocha-lcov-reporter": "1.3.0",
    "mocha-reporter": "0.0.3",
    "mysql": "2.5.4",
    "npm-run-all": "4.0.2",
    "plato": "1.7.0",
    "power-assert": "1.4.2",
    "retire": "1.3.1",
    "sqlite3": "3.1.8",
    "typedoc": "0.7.0",
    "typedoc-plugin-sourcefile-url": "1.0.3",
    "typescript": "2.3.2",
    "yamljs": "0.2.10",
    "zmq": "2.15.3"
  },
  "main": "lib/mage",
  "repository": {
    "type": "git",
    "url": "https://github.com/mage/mage.git"
  },
  "licence": "MIT"
}
